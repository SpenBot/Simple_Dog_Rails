<h2>ADD NEW DOG PROFILE</h2>


<p> Enter in your new dog's information. </p>

<%= form_for @dog do |f| %>

  </br>
  <%= f.label :"DOG'S NAME" %>
  <%= f.text_field :name %>
  </br>
  </br>
  <%= f.label :"PHOTO" %>
  <%= f.text_field :photo_url %>
  </br>
  </br>
  <%= f.label :"OWNER FULL NAME" %>
  <%= f.text_field :owner %>
  </br>
  </br>
  <%= f.label :"BREED" %>
  <%= f.text_field :breed %>
  </br>
  </br>
  <%= f.label :"WEIGHT" %>
  <%= f.text_field :weight %>
  </br>
  </br>
  <%= f.label :"MEDICAL CHECK" %>
  <%= f.text_field :medical_check %>
  </br>
  </br>
  <%= f.submit %>

<% end %>
